{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2",
    "enable_experimental_live_diffs": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Base16 Default Dark"
  },
  "vim": {
    "use_smartcase_find": true
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "buffer_font_family": "Inconsolata for Powerline",
  "env": {
    "EDITOR": "zed"
  },
  "lsp": {
    "gopls": {
      "initialization_options": {
        "completeFunctionCalls": false
      }
    }
  }
}
